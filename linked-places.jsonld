{
	"@context": {
		// begin GeoJSON prefixes
    "geojson": "https://purl.org/geojson/vocab#",
    "Feature": "geojson:Feature",
    "FeatureCollection": "geojson:FeatureCollection",
    "GeometryCollection": "geojson:GeometryCollection",
    "LineString": "geojson:LineString",
    "MultiLineString": "geojson:MultiLineString",
    "MultiPoint": "geojson:MultiPoint",
    "MultiPolygon": "geojson:MultiPolygon",
    "Point": "geojson:Point",
    "Polygon": "geojson:Polygon",
    "bbox": {
      "@container": "@list", "@id": "geojson:bbox"
    },
    "coordinates": {
      "@container": "@list", "@id": "geojson:coordinates"
    },
    "features": {
      "@container": "@set",  "@id": "geojson:features"
    },
    "geometry": "geojson:geometry",
    "properties": "geojson:properties",
		// end GeoJSON prefixes

    "aat": "http://vocab.getty.edu/aat/",
		"cc": "http://creativecommons.org/licenses/",
		"cito": "http://purl.org/spar/cito",
		"cnt": "http://www.w3.org/2011/content#",
		"crm": "http://www.cidoc-crm.org/cidoc-crm/",
    "dc": "http://purl.org/dc/elements/1.1/",
		"dcterms": "http://purl.org/dc/terms/",
		"foaf": "http://xmlns.com/foaf/0.1/",
		"gn": "http://www.geonames.org/ontology#",
		"gvp": "http://vocab.getty.edu/ontology#",
		"lawd": "http://lawd.info/ontology/",
		"lp": "http://linkedpasts.org/vocab#", // 'when', ?
		"rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
		"rdfs": "http://www.w3.org/2000/01/rdf-schema#",
		"skos": "http://www.w3.org/2004/02/skos/core#",

		"id:": "@id", // always a uri
		"type": "@type",
		"identifier": "dcterms:identifier",
    "label": {"@id": "rdfs:label"},
		"title": {"@id": "dcterms:title"},
		"when": {"@id": "lp:when"},
		"descriptions": {
			"@id": "dcterms:description",
			"@type": "@id",
			"@container": "@set"
		},
		"attestations": {
			"@id": "lawd:hasAttestation",
			"@type": "@id",
			"@container": "@set"
		},
		"depictions": {
			"@id": "foaf:depiction",
			"@type": "@id",
			"@container": "@set"
		},
		"names": {
			"@id": "lawd:hasName",
			"@type": "@id",
			"@container": "@set"
		},
		"place_type": {
			"@id": "gvp:placeTypePreferred",
			"@type": "@id",
			"@container": "@set"
		},
		"part_of": {
			"@id": "gvp:broaderPartitive",
			"@type": "@id",
			"@container": "@set"
		},
    "related": {
    	"@id": "dcterms:relation",
    	"@type": "@id",
    	"@container": "@set"
    },
		"primary_topic_of": {
			"@id": "foaf:isPrimaryTopicOf",
			"@type":"@id"
			},
    "subject_of": {
      "@id": "crm:P129i_is_subject_of",
      "@type": "@id"
    },
		"close_match:": "skos:closeMatch",
		"exact_match:": "skos:exactMatch",
		"see_also": "rdfs:seeAlso",
		"value": {"@id": "rdf:value"},
		"license": "dcterms:license",
	}
}